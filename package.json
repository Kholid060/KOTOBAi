{
  "name": "chrome-extension-boilerplate-react-vite",
  "version": "0.0.1",
  "description": "chrome extension boilerplate",
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "yarn build:hmr && (run-p wss build:watch)",
    "dev:firefox": "yarn build:hmr && (run-p wss build:firefox:watch)",
    "test": "jest",
    "commitlint": "commitlint --edit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install",
    "generate-data": "node ./utils/scripts/generate-data.js"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/react": "^0.26.3",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@types/webextension-polyfill": "^0.10.6",
    "axios": "^1.6.2",
    "construct-style-sheets-polyfill": "3.1.0",
    "dexie": "^3.2.4",
    "events": "^3.3.0",
    "fetch-retry": "^5.0.6",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "type-fest": "^4.8.2",
    "usehooks-ts": "^2.9.1",
    "wanakana": "^5.3.1",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@rollup/plugin-typescript": "11.1.5",
    "@testing-library/react": "14.0.0",
    "@types/chrome": "0.0.251",
    "@types/jest": "29.5.7",
    "@types/node": "20.8.10",
    "@types/pako": "^2.0.3",
    "@types/react": "18.2.35",
    "@types/react-dom": "18.2.14",
    "@types/ws": "8.5.8",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "@vitejs/plugin-react": "2.2.0",
    "autoprefixer": "^10.4.16",
    "chokidar": "3.5.3",
    "commander": "^11.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.53.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "libxmljs": "^1.0.11",
    "lint-staged": "15.0.2",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.31",
    "prettier": "3.0.3",
    "rollup": "4.3.0",
    "semver": "^7.5.4",
    "tailwindcss": "^3.3.5",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "^5.0.2",
    "ws": "8.14.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
